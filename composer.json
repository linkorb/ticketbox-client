{
    "name": "linkorb/ticketbox-client",
    "description": "TicketBox Client for TicketBox Application",
    "homepage": "https://github.com/linkorb/ticketbox-client",
    "keywords": ["ticketbox", "client-library", "php", "api", "linkorb"],
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "LinkORB",
        "email": "engineering@linkorb.com"
    },{
        "name": "Kishan Thobhani",
        "email": "thobhanikishan@gmail.com"
    }],
    "require": {
        "php": ">=5.3.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "linkorb/conventions-checker": "^1.1"
    },
    "extra": {
        "grumphp": {
            "config-default-path": "vendor/linkorb/conventions-checker/config/lib/grumphp.yml"
        }
    },
    "autoload": {
        "psr-4" : {
            "TicketBox\\Client\\": "src/"
        }
    }
}
